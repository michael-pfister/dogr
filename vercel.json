{
    "version": 2,
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/now-builder",
        "config": {
          "serverRuntimeConfig": {},
          "publicRuntimeConfig": {}
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/nuxt-ssr-bundle/$1"
      }
    ]
  }
  